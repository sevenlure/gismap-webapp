{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["styled-components", { "ssr": true }]
  ]
}
